if [[ ${ZSHENV_LOADED:-BLA} = 1 ]]; then
   return
fi

[[ -s ~/.envvars.rc ]] && . ~/.envvars.rc

export PATH=/usr/local/opt/ccache/libexec:${DOTFILESDIR}/bin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:/usr/local/sbin:$PATH

# Unicode FTW
export LC_ALL=en_US.UTF-8
export LANG=en_US.UTF-8

export ZSHENV_LOADED=1
